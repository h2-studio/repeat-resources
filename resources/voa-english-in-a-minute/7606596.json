[{"start":4.17,"end":5.76,"text":"Welcome to English in a Minute!","phonetics":[[0,7,"'wɛlkəm"],[11,7,"'ɪŋglɪʃ"],[24,6,"'mɪnɪt"]]},{"start":6.57,"end":7.95,"text":"This is a belt.","phonetics":[[0,4,"ðɪs"],[10,4,"bɛlt"]]},{"start":8.37,"end":10.35,"text":"You wear it around your waist.","phonetics":[[0,3,"ju"],[4,4,"wɜr"],[12,6,"ə'raʊnd"],[19,4,"jɔr"],[24,5,"weɪst"]]},{"start":10.86,"end":13.59,"text":"But what could under your belt mean?","phonetics":[[0,3,"bət"],[4,4,"wʌt"],[9,5,"kʊd"],[15,5,"'əndər"],[21,4,"jɔr"],[26,4,"bɛlt"],[31,4,"min"]]},{"start":14.7566666666667,"end":16.9236666666667,"text":"I tried making baguettes for my mom's birthday","phonetics":[[2,5,"traɪd"],[8,6,"'meɪkɪŋ"],[15,9,"bæ'ɡɛtz"],[25,3,"fɔr"],[38,8,"'bɜrθ,deɪ"]]},{"start":17.82,"end":18.87,"text":"It did not go well.","phonetics":[[3,3,"dɪd"],[7,3,"nɑt"],[14,4,"wɛl"]]},{"start":19.47,"end":21.96,"text":"Dan, why didn't you ask me for help?","phonetics":[[0,3,"dæn"],[5,3,"hwaɪ"],[9,6,"'dɪdʌnt"],[16,3,"ju"],[20,3,"æsk"],[27,3,"fɔr"],[31,4,"hɛlp"]]},{"start":22.38,"end":25.326,"text":"I have years of baguette baking under my belt.","phonetics":[[2,4,"hæv"],[7,5,"jɪrz"],[16,8,"bæ'ɡɛt"],[25,6,"'beɪkɪŋ"],[32,5,"'əndər"],[41,4,"bɛlt"]]},{"start":25.98,"end":27.15,"text":"Baguettes under your belt?","phonetics":[[0,9,"bæ'ɡɛtz"],[10,5,"'əndər"],[16,4,"jɔr"],[21,4,"bɛlt"]]},{"start":27.9465833333333,"end":28.4865833333333,"text":"That's funny!","phonetics":[[0,6,"ðæts"],[7,5,"'fəni"]]},{"start":29.21625,"end":30.50625,"text":"This is no joke, Dan","phonetics":[[0,4,"ðɪs"],[11,4,"dʒoʊk"],[17,3,"dæn"]]},{"start":30.6625833333333,"end":34.2925833333333,"text":"Do you want my years of experience baking baguettes, or not?","phonetics":[[3,3,"ju"],[7,4,"wɑnt"],[15,5,"jɪrz"],[24,10,"ɪk'spiriəns"],[35,6,"'beɪkɪŋ"],[42,9,"bæ'ɡɛtz"],[56,3,"nɑt"]]},{"start":34.8064166666667,"end":35.9074166666667,"text":"Sorry, yes.","phonetics":[[0,5,"'sɑri"],[7,3,"jɛs"]]},{"start":36.5995,"end":37.5565,"text":"No, no joke.","phonetics":[[7,4,"dʒoʊk"]]},{"start":39.6087651515152,"end":44.9807651515152,"text":"Anything under your belt is something you've already achieved or learned.","phonetics":[[0,8,"'ɛni,θɪŋ"],[9,5,"'əndər"],[15,4,"jɔr"],[20,4,"bɛlt"],[28,9,"'sʌmθɪŋ"],[38,6,"juv"],[45,7,"ɔl'rɛdi"],[53,8,"ʌ'tʃivd"],[65,7,"lɜrnd"]]},{"start":45.4544242424242,"end":51.7244242424242,"text":"For example, I have many many English in a Minute episodes under my belt.","phonetics":[[0,3,"fɔr"],[4,7,"ɪɡ'zæmpəl"],[15,4,"hæv"],[20,4,"'mɛni"],[25,4,"'mɛni"],[30,7,"'ɪŋglɪʃ"],[43,6,"'mɪnɪt"],[50,8,"'ɛpɪ,soʊd"],[59,5,"'əndər"],[68,4,"bɛlt"]]},{"start":52.1154545454545,"end":54.0954545454545,"text":"So, I know my idioms.","phonetics":[[6,4,"noʊ"],[14,6,"'ɪdiəms"]]},{"start":54.8986060606061,"end":57.5086060606061,"text":"And that's another English in a Minute!","phonetics":[[0,3,"ænd"],[4,6,"ðæts"],[11,7,"ə'nəðər"],[19,7,"'ɪŋglɪʃ"],[32,6,"'mɪnɪt"]]}]
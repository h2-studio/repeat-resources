[{"start":4.32,"end":5.88,"text":"Welcome to English in a Minute!","phonetics":[[0,7,"'wɛlkəm"],[11,7,"'ɪŋglɪʃ"],[24,6,"'mɪnɪt"]]},{"start":6.39,"end":8.13,"text":"This is a punch.","phonetics":[[0,4,"ðɪs"],[10,5,"pənʧ"]]},{"start":8.64,"end":11.76,"text":"So, what does it mean to pull punches?","phonetics":[[4,4,"wʌt"],[9,4,"dʌz"],[17,4,"min"],[25,4,"pʊl"],[30,7,"pənʧ"]]},{"start":12.870111,"end":14.999643,"text":"Dan, I am so mad at Bob.","phonetics":[[0,3,"dæn"],[13,3,"mæd"],[20,3,"bɑb"]]},{"start":15.27,"end":18.9,"text":"He promised to bring the main dish to the potluck party","phonetics":[[3,8,"'prɑmʌst"],[15,5,"brɪŋ"],[21,3,"ðʌ"],[25,4,"meɪn"],[30,4,"dɪʃ"],[38,3,"ðʌ"],[42,7,"'pɑt,lək"],[50,5,"'pɑrti"]]},{"start":19.23,"end":20.403,"text":"and then forgot!","phonetics":[[0,3,"ænd"],[4,4,"ðɛn"],[9,6,"fɝr'ɡɑt"]]},{"start":20.91,"end":22.95,"text":"Well, we all forget things.","phonetics":[[0,4,"wɛl"],[9,3,"ɔl"],[13,6,"fər'ɡɛt"],[20,6,"θɪŋz"]]},{"start":23.4395833333333,"end":25.0500833333333,"text":"Not the main dish, Dan!","phonetics":[[0,3,"nɑt"],[4,3,"ðʌ"],[8,4,"meɪn"],[13,4,"dɪʃ"],[19,3,"dæn"]]},{"start":25.0740833333333,"end":27.6095833333333,"text":"he ruined the potluck party for everyone","phonetics":[[3,6,"'ruʌnd"],[10,3,"ðʌ"],[14,7,"'pɑt,lək"],[22,5,"'pɑrti"],[28,3,"fɔr"],[32,8,"'ɛvri,wʌn"]]},{"start":27.632,"end":28.439875,"text":"And I told him.","phonetics":[[0,3,"ænd"],[6,4,"toʊld"],[11,3,"hɪm"]]},{"start":28.53,"end":28.95,"text":"Wow.","phonetics":[[0,3,"waʊ"]]},{"start":29.46,"end":31.95,"text":"You really don't pull any punches, do you?","phonetics":[[0,3,"ju"],[4,6,"'riəli"],[11,5,"doʊnt"],[17,4,"pʊl"],[22,3,"'ɛni"],[26,7,"pənʧ"],[38,3,"ju"]]},{"start":32.34,"end":33.75,"text":"Not when it comes to my potluck.","phonetics":[[0,3,"nɑt"],[4,4,"wɛn"],[12,5,"kʌmz"],[24,7,"'pɑt,lək"]]},{"start":35.4824166666667,"end":40.1614166666667,"text":"Someone who doesn't pull any punches speaks and acts directly,","phonetics":[[0,7,"'səmər"],[8,3,"hu"],[12,7,"'dʌzʌnt"],[20,4,"pʊl"],[25,3,"'ɛni"],[29,7,"pənʧ"],[37,6,"spik"],[44,3,"ænd"],[48,4,"ækts"],[53,8,"dɪ'rɛktli"]]},{"start":40.6133333333333,"end":43.8233333333333,"text":"even if their actions or words might upset someone.","phonetics":[[0,4,"'ivən"],[8,5,"ðɛr"],[14,7,"'ækʃən"],[25,5,"wɝrdz"],[31,5,"maɪt"],[37,5,"əp'sɛt"],[43,7,"'səmər"]]},{"start":44.58,"end":46.35,"text":"This phrase comes from boxing.","phonetics":[[0,4,"ðɪs"],[5,6,"freɪz"],[12,5,"kʌmz"],[18,4,"frʌm"],[23,6,"'bɑksɪŋ"]]},{"start":46.98,"end":48.9,"text":"When boxers pull a punch,","phonetics":[[0,4,"wɛn"],[5,6,"'bɑksɝrz"],[12,4,"pʊl"],[19,5,"pənʧ"]]},{"start":49.14,"end":51.18,"text":"they don't hit as hard as they can.","phonetics":[[0,4,"ðeɪ"],[5,5,"doʊnt"],[11,3,"hɪt"],[18,4,"hɑrd"],[26,4,"ðeɪ"],[31,3,"kæn"]]},{"start":51.6,"end":53.73,"text":"They take it easy on their opponent,","phonetics":[[0,4,"ðeɪ"],[5,4,"teɪk"],[13,4,"'izi"],[21,5,"ðɛr"],[27,8,"ə'poʊnənt"]]},{"start":54.2863333333333,"end":55.1263333333333,"text":"unlike Anna.","phonetics":[[0,6,"ən'laɪk"],[7,4,"'ænə"]]},{"start":56.1756666666667,"end":57.7656666666667,"text":"And that's English in a Minute.","phonetics":[[0,3,"ænd"],[4,6,"ðæts"],[11,7,"'ɪŋglɪʃ"],[24,6,"'mɪnɪt"]]}]